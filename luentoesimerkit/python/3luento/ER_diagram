digraph {
	tests [label="tests
------------
test_number (PK)
test_timestamp
doctors_reference
creator (FK)
"]
	customers [label="customers
------------
customer_number (PK)
first_name
last_name
personid"]
	employees [label="employees
------------
creator_number (PK)
"]
	measures [label="measures
------------
test_number (PK, FK)
"]
	tests -> customers [label="customer_number (FK)"]
	tests -> employees [label="creator (FK)"]
	tests -> measures [label="test_number (FK)"]
}
